{
    "name": "joselfonseca/laravel-api",
    "description": "Laravel API starter Kit will give you most of the boilerplate that you need for creating Laravel API's",
    "keywords": ["framework", "laravel", "Api", "REST", "dingo", "oAuth2"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Jose Fonseca",
            "email": "jose@ditecnologia.com",
            "homepage": "https://josefonseca.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "joselfonseca/laravel-tactician" : "0.3.*",
        "joselfonseca/laravel-api-tools": "^2.2",
        "laravel/passport": "^2.0",
        "webpatser/laravel-uuid": "^2.0",
        "spatie/laravel-permission": "^1.7"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
